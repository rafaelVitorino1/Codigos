<script>

var carro, estrada, limite;

estrada = Number(prompt("Digite a velocidade permitida"));
carro = Number(prompt("Informe a velocidade do veiculo"));
limite = (estrada*0.20)+estrada;


if(carro>=estrada && carro<limite){

    alert("Multa leve")

}else if(carro>limite){

    alert("MULTA GRAVE")

}else{

    alert("Sem multa")
    
}

</script>
